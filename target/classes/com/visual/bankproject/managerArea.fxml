<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.Tab?>

<?import java.net.URL?>
<TabPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2"
         xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="com.visual.bankproject.ManagerAreaController" styleClass="tabpane-bg">
    <Tab fx:id="clientsTab" closable="false" text="My clients">
        <fx:include source="tabs/managerTabs/my-clients.fxml"/>
    </Tab>
    <Tab fx:id="accountInfoTab" closable="false" text="Account Info">
        <fx:include source="tabs/managerTabs/account-info.fxml"/>
    </Tab>
    <Tab fx:id="depositToClientTab" closable="false" text="Investment">
        <fx:include source="tabs/managerTabs/investment.fxml"/>
    </Tab>
    <Tab fx:id="transferTab" closable="false" text="Transfer">
        <fx:include source="tabs/managerTabs/transfer.fxml"/>
    </Tab>
    <Tab fx:id="limitTab" closable="false" text="Withdraw">
        <fx:include source="tabs/managerTabs/withdraw.fxml"/>
    </Tab>
    <Tab fx:id="newAccountTab" closable="false" text="New Account">
        <fx:include source="tabs/managerTabs/new-account.fxml"/>
    </Tab>
    <Tab fx:id="changePasswordTab" closable="false" text="Change Password">
        <fx:include source="tabs/managerTabs/change-password.fxml"/>
    </Tab>

    <stylesheets>
        <URL value="@styles/tabs.css"/>
    </stylesheets>
</TabPane>

