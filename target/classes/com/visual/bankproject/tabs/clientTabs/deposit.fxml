<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane prefHeight="438.0" prefWidth="557.0" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visual.bankproject.ClientDepositController">

    <center>
        <VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="15.0">

            <ChoiceBox maxWidth="200" prefHeight="35" fx:id="accountOptions">

            </ChoiceBox>

            <Label text="Type the amount you want to deposit" styleClass="labelColor">
                <VBox.margin>
                    <Insets bottom="-12.0"/>
                </VBox.margin>
            </Label>
            <TextField fx:id="amountValue" maxWidth="200" prefHeight="35"/>

            <Button text="Deposit" styleClass="buttons" onAction="#onDepositButtonClick" fx:id="depositButton">
                <VBox.margin>
                    <Insets top="20.0"/>
                </VBox.margin>
            </Button>
        </VBox>
    </center>
</BorderPane>