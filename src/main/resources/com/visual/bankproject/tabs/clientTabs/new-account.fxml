<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="438.0" prefWidth="557.0" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visual.bankproject.ClientNewAccountController">
    <center>
        <VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="15.0">
            <Label text="Choose your account type" fx:id="accountTypeMessage"/>

            <Button maxWidth="200" prefHeight="35" styleClass="buttons" text="Simple Account" onAction="#createSimpleAccount"/>
            <Button maxWidth="200" prefHeight="35" styleClass="buttons" text="Savings Account" onAction="#createSavingAccount"/>
            <Button maxWidth="200" prefHeight="35" styleClass="buttons" text="Special Account" onAction="#createSpecialAccount"/>
        </VBox>
    </center>

</BorderPane>